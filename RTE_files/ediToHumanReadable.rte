inline
double	nfFindPos(char *toFind, char **ptr)
{
  int	i = 0;

  while (ptr[i] != NULL && strcmp(ptr[i], toFind) != 0)
    i++;
  return ((ptr[i] != NULL) ? i : 0);
}
